  },
  "license": "MIT",
  "keywords": [
    "pridepack"
  ],
  "name": "react-compostate",
  "devDependencies": {
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "compostate": "0.3.0",
    "eslint": "^8.15.0",
    "eslint-config-lxsmnsyc": "^0.4.0",
    "pridepack": "^1.1.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-test-renderer": "^18.1.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "compostate": "^0.2.1-beta.0",
    "react": "^16.8 || ^17.0 || ^18.0",
    "react-dom": "^16.8 || ^17.0 || ^18.0"
